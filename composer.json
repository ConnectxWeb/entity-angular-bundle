{
    "name": "connectx/entity-to-angular",
    "type": "symfony-bundle",
    "description": "Symfony bundle to convert entities into Angular TS files.",
    "keywords": ["entity", "angular", "converter"],
    "homepage": "https://github.com/ConnectxWeb/entity-to-angular",
    "license": "MIT",
    "authors": [
        {
            "name": "Benjamin Leveque",
            "email": "ben@connectx.fr"
        },
        {
            "name": "Connectx",
            "homepage": "https://github.com/ConnectxWeb"
        }
    ],
    "require": {
        "php": ">=5.1",
        "symfony/framework-bundle": "^2.8 || ^3.0 || ^4.0",
        "symfony/validator": "^2.8 || ^3.0 || ^4.0"
    },
    "require-dev": {
        "symfony/console": "^2.8 || ^3.0 || ^4.0",
        "symfony/phpunit-bridge": "^2.8 || ^3.0 || ^4.0",
        "symfony/yaml": "^2.8 || ^3.0 || ^4.0",
        "phpunit/phpunit": "^4.8.35|^5.7.11|^6.5"
    },
    "conflict": {
        "doctrine/doctrine-bundle": "<1.3",
        "symfony/doctrine-bridge": "<2.7"
    },
    "autoload": {
        "psr-4": {
            "Connectx\\EntityAngularBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "extra": {
        "branch-alias": {
        }
    },
    "support": {
        "docs": "https://github.com/ConnectxWeb/entity-to-angular"
    }
}
